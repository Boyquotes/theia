{
  "private": true,
  "name": "plugin-a",
  "version": "1.42.0",
  "main": "extension.js",
  "license": "EPL-2.0 OR GPL-2.0-only WITH Classpath-exception-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-theia/theia.git"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "devDependencies": {
    "@types/vscode": "^1.50.0"
  },
  "scripts": {
    "prepare": "yarn -s package",
    "package": "vsce package --yarn"
  },
  "contributes": {
    "commands": [
      {
        "command": "plugin-a.hello",
        "title": "Hello from plugin-a"
      }
    ],
    "languages": [
      {
        "id": "python",
        "extensions": [
          ".py",
          ".python"
        ],
        "icon": {
          "dark": "resources/python_dark.svg",
          "light": "resources/python_light.svg"
        }
      }
    ]
  }
}
